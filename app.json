{
  "expo": {
    "name": "FlavorMind MVP 2",
    "slug": "flavormind-mvp2",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow FlavorMind to access your photos to add meal images.",
          "cameraPermission": "Allow FlavorMind to access your camera to take photos of your meals."
        }
      ]
    ],
    "sdkVersion": "52.0.0",
    "newArchEnabled": true
  }
}
